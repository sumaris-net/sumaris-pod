            <column name="PMFM_STRATEGY_FK" valueComputed="(SELECT ID FROM PMFM_STRATEGY WHERE RANK_ORDER=-1)"/>
            <column name="TAXON_GROUP_FK" valueComputed="(SELECT ID FROM TAXON_GROUP WHERE TAXON_GROUP_TYPE_FK=2 AND LABEL='MNZ')"/>
