<queries name="createMonitoringTable">

    <query type="create" temp="false" table="&amp;resultMonitoringTableName">

        <select type="number" alias="ACTIVITY_CALENDAR_ID">&amp;monitoringTableName.ACTIVITY_CALENDAR_ID</select>
        <select type="text" alias="VESSEL_FEATURES_NAME">&amp;monitoringTableName.VESSEL_FEATURES_NAME</select>
        <select type="text" alias="REGISTRATION_LOCATION_LABEL">&amp;monitoringTableName.REGISTRATION_LOCATION_LABEL</select>
        <select type="text" alias="VESSEL_FEATURES_LENGHT">&amp;monitoringTableName.VESSEL_FEATURES_LENGHT</select>
        <select type="text" alias="SURVEY_QUALIFICATION">&amp;monitoringTableName.SURVEY_QUALIFICATION</select>
        <select type="text" alias="RECORDER_PERSON">&amp;monitoringTableName.RECORDER_PERSON</select>
        <from>&amp;monitoringTableName</from>
    </query>

</queries>
